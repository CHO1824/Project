**/
this.fn_afterSaveCheck = function(ErrorCode, ErrorMsg, TrId)
{	 
	if(ErrorCode < 0) {
		comSys.showMsg("E0001", "W", ["확인"]);		
	} else {
		
		var returnMsg = this.ds_out_saveChk.getColumn(this.ds_out_saveChk.rowposition, "오류코드");    //00 이면 정상
		
		if (Base.isEmpty(returnMsg) ) {
			comSys.showMsg("등록불가 체크 중 에러 발생 - 전산실에 요청", "E", [""]);
			return false;
		}
		
		var errCode   = returnMsg.substr(0,2);
		var errMsg    = returnMsg.substr(3);
		
		if (errCode != "00") {
			
			comSys.showMsg("오류코드 : "+errCode+"\n"+errMsg, "E", [""]);
			return false;
		}
		
		//저장
		this.fn_dataSave("U");	
	}
};

