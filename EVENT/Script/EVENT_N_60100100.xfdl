/**
* 사원찾기팝업
**/
this.fn_empSeachPop = function(gbn, empGbn)
{
	var oArgs = {};
	var pv_sReturnType, pv_selectType, pv_sawonNm;	
	if (empGbn == "S"){
		var empName = comSys.transNullToEmpty(this.div_search.form.edt_empNm.value);
	}else if(empGbn == "B"){
		var empName = comSys.transNullToEmpty(this.div_search.form.edt_empNm00.value);
	}
	if(gbn == "btn"){
		oArgs = {
			pv_sReturnType : "A"
			,pv_selectType : "auto"
			,pv_sawonNm : empName
		};
	}
	comSys.openPopup("com::COM_P_0000008.xfdl", empGbn, oArgs, null, this, "fn_popupCallBack");
}

this.fn_popupCallBack = function(popupId, rtn)
{
	var aJson;
	if(Base.isEmpty(rtn)) return;
	try{
		aJson = JSON.parse(rtn);
	}catch(ex){}
	
	var empName = comSys.transNullToEmpty(this.div_search.form.edt_empNm.value);
	switch(popupId){
		case "S":
			empName = comSys.transNullToEmpty(this.div_search.form.edt_empNm.value);
			break;
		case "B":
			empName = comSys.transNullToEmpty(this.div_search.form.edt_empNm00.value);
			break;
	}
	
	switch(popupId){
		case "S":
			this.div_search.form.edt_empNo.set_value(aJson[0]);
			this.div_search.form.edt_empNm.set_value(aJson[1]);
			break;
		case "B":
			this.div_search.form.edt_empNo00.set_value(aJson[0]);
			this.div_search.form.edt_empNm00.set_value(aJson[1]);
			break;
	}
};
