this.ds_burial_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "행사시작일자"){
		var endDate = this.ds_burial.getColumn(e.row,"행사종료일자");
		if(Base.isEmpty(endDate)) return;
		
		if(e.newvalue > endDate){
			comSys.showMsg("시작일자는 종료일자보다 작아야합니다.","I");
			return false;
		}
	}
	else if(e.columnid == "행사종료일자"){
		var startDate = this.ds_burial.getColumn(e.row,"행사시작일자");
		if(Base.isEmpty(startDate)) return;
		
		if(e.newvalue < startDate){
			comSys.showMsg("종료일자는 시작일자보다 커야합니다.","I");
			return false;
		}
	}else if(e.columnid == "입관일자"){
		var startDate = this.ds_burial.getColumn(e.row,"행사시작일자");
		var endDate = this.ds_burial.getColumn(e.row,"행사종료일자");
		if(Base.isEmpty(startDate) || Base.isEmpty(endDate)) return;
		
		if(e.newvalue < startDate){
			comSys.showMsg("입관일자는 시작일자보다 커야합니다.", "I");
			return false;
		}
		
		if(e.newvalue > endDate){
			comSys.showMsg("입관일자는 종료일자보다 작아야합니다.","I");
			return false;
		}
		
	}
	
}
