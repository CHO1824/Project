/**
 * 저장 함수
 **/
this.fn_dataSave = function(gbn)
{
	var eventST = this.ds_burial.getColumn(this.ds_burial.rowposition,"행사상태");
	if(eventST == "접수"){
		this.ds_burial.setColumn(this.ds_burial.rowposition,"행사상태","진행");
	}
	
	var progress = this.ds_burial.getColumn(this.ds_burial.rowposition, "상품매출");							//행사진행금액
	var discount = nexacro.toNumber(this.ds_burial.getColumn(this.ds_burial.rowposition, "행사특별할인"),0);	//할인금액
	var sales = progress - discount;

	this.fsp_clear();
	this.fsp_addSave(
		  ""									// insert
		, fv_fspGroup+"event:EVENT_N_60100100_"+gbn	// update
		, ""									// delete
		, ""									// flag column 명
		, ""									// key sql id
		, 0										// key 증가 값
		, "" 
		, 0 
		, "N"									// 실행타입
	);
	
	// 서버 호출 
	this.fsp_callService(
		""
		,""
		,"ds_input=ds_burial:U"		// inDs
		,""							// outDs
		,"sales="+sales				// args
		,"fn_afterSave"
		,false						//에러결과		true : 호출X, false : 호출o
		,true						//처리중표시	true : 표시x, false : 표시o
		,false						//동기비동기	true : 동기, false : 비동기
	);
}

