	this.ds_resources.setColumn(this.ds_resources.rowposition, "단가", price);			//단가
	this.ds_resources.setColumn(this.ds_resources.rowposition, "작업금액", totalSum);	//작업수당
	this.ds_resources.setColumn(this.ds_resources.rowposition, "총액", totalSum);		//총액
	this.ds_resources.setColumn(this.ds_resources.rowposition, "세액", amountSum);		//세액
	this.ds_resources.setColumn(this.ds_resources.rowposition, "실지급액", paymentMin);	//지급액
};

