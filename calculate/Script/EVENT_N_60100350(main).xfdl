// 인력 탭 작업시간 계산
this.fn_onchanged = function(obj:nexacro.MaskEdit,e:nexacro.ChangeEventInfo)
{
	var work = this.Tab00.tabpage4.form.div_search.form.mae_quantity1.value;		//작업시간
	var price = this.Tab00.tabpage4.form.div_search.form.mae_quantity2.value;		//단가
