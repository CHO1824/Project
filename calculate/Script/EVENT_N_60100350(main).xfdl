this.fn_onkeyup = function(obj:nexacro.MaskEdit,e:nexacro.KeyEventInfo)
{
	var oneQuantity = this.Tab00.tabpage3.form.div_search.form.edt_quantity1.value;
	var oneSale = this.Tab00.tabpage3.form.div_search.form.mae_quantity1.value;
	var oneSum = oneQuantity * oneSale;
	
	var twoQuantity = this.Tab00.Tabpage1.form.div_search.form.edt_quantity1.value;
	var twoSale = this.Tab00.Tabpage1.form.div_search.form.mae_quantity2.value;
	var twoSum = twoQuantity * twoSale;
	
	var conEmpNM = this.div_search.form.edt_ConEmpNM.value;
	var pv_sawonNm =this.Tab00.tabpage4.form.div_search.form.edt_resourcesNM.value;
	
	if (obj.name == "mae_quantity1" && e.keycode == "13"){
