//장례정산 확정 / 취소
this.divDetail01_btn_01_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var Fix_Date = this.ds_information.getColumn(this.ds_information.rowposition,"정산완료일");
	var eventCD = this.ds_list.getColumn(this.ds_list.rowposition, "행사코드");
	
	this.ds_in_Fix.clearData();
	var nRow = this.ds_in_Fix.addRow();
	
	if(Base.isEmpty(eventCD)){
		comSys.showMsg("행사정보를 선택해주세요.","I");
		return;
	}
	
	if(obj.name == "btn_01"){
		if(Base.isEmpty(Fix_Date)){
			comSys.showMsg("정산완료일은 필수값입니다.","I");
			this.cal_approval.setFocus();
			return;
		}
			this.ds_in_Fix.setColumn(nRow,"행사상태","완료");
	}else{
		if(Base.isEmpty(Fix_Date)){
			comSys.showMsg("미완료된 행사입니다.","I");
