this.fn_del = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var eventCD = this.ds_list.getColumn(this.ds_list.rowposition, "행사코드");
	if(Base.isEmpty(eventCD)){
		comSys.showMsg("행사정보를 선택해주세요.","I");
		return;
	}
	var btnName = obj.name;
	var ds;
	var cRow;
	
	if (btnName == "btn_del1") {
		ds = this.ds_basics;
		cRow = ds.rowposition;
	}else if (btnName == "btn_del2"){
		ds = this.ds_additional;
		cRow = ds.rowposition;
	}else if (btnName == "btn_del3"){
		ds = this.ds_resources;
		cRow = ds.rowposition;
	}else if (btnName == "btn_del4"){
		ds = this.ds_content;
		cRow = ds.rowposition;
	}
	
	if(cRow == -1) {
		comSys.showMsg("삭제할 행을 선택해주세요.","E","");
		return;
	} // End of if
    
	if(ds.getRowType(cRow) != 2){
		if(!comSys.showMsg("C0009","Q",["※ 삭제 후 저장은 필수입니다."])) {
			return;
		} // End of if
	}
	
	ds.deleteRow(cRow);
};

