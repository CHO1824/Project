		this.ds_calc.setColumn(Row, "현장경비", expense1);
		this.ds_calc.setColumn(Row, "실입금액", actuality);
		this.ds_calc.setColumn(Row, "자체용품금액", office);
		this.ds_calc.setColumn(Row, "부금납입", payment);
	}
	this.fn_calculate();
	
};

//장례정산 집계 계산
this.fn_calculate = function()
{
	/*
		계산공식
		(매출:4번=1번-3번) 					 	:= (진행상품금액:1번) - (할인(할인상주공제):3번)			-- (4)
		(잔여금액(카드수입총액):7번=4번-6번)	 := (매출:4번)  - (부금(납구금):6번)						-- (7)
		(회사입금액:9번=7번-8번) 			 	:= (잔여금액(카드수입총액):7번) - (현장지출:8번)			-- (9)
		(현장지출 : 2.정산) 				 	:= (기본제공비용 tab : 기본제공 탭 현장지출)				--현장지출 2.정산
		(현장지출 : 1.장례식장 행사조건) 	 	:= (기본제공비용 tab : 기본제공 탭 현장지출)				--현장지출 1.장례식장 행사조건
		(최종입금액 : 10번=9번+2번) 		 	:= (회사입금액:9번)  + (추가금액:2번)						--(10)
		(차액 : 12번=10번-11번) 			 	:= (최종입금액 : 10번) - (실입금액(실제입금액) : 11번)		--(12)
		(매출총액 : 13번=4번+2번)    		 	:= (매출:4번)  + (추가금액:2번)								--(13)
		(총비용(총지출경비) : 2.정산) 		 	:= (본사지출 : 2.정산) + (현장지출 : 2.정산)				--총비용 2.정산
		(지출비율(상품액대비 지출비율 : 2.정산)	:= (총비용(총지출경비) : 2.정산) / (매출:4번) * 100			--상품액 대비 지출비율 2.정산
	*/
	
	var progress = this.ds_list.getColumn(this.ds_list.rowposition, "상품매출");											//행사진행금액
	var discount = nexacro.toNumber(this.ds_list.getColumn(this.ds_list.rowposition, "행사특별할인"),0);					//할인금액
	var expense1 = nexacro.toNumber(this.ds_basics.getColumn(this.ds_basics.rowposition, "현장지출"),0);					//현장경비
	var additional = nexacro.toNumber(this.ds_calc.getColumn(this.ds_calc.rowposition, "추가금액"),0);						//추가금액
	var actuality = nexacro.toNumber(this.ds_list.getColumn(this.ds_list.rowposition, "실입금액"),0);						//실입금액
	var office = nexacro.toNumber(this.ds_basics.getColumn(this.ds_basics.rowposition, "본사지출"),0);						//본사지출
	var payment = nexacro.toNumber(this.ds_list.getColumn(this.ds_list.rowposition, "부금납입"),0);							//부금납입
	
	var sales = progress - discount;		//매출 (4)
	var remaining = sales - payment;		//잔여금액 (7)
	var deposit = remaining - expense1;		//회사입금액 (9)
	var final = deposit + additional;		//최종입금액 (10)
	var difference = final - actuality;		//차액 (12)
	var total = sales + additional;			//매출총액 (13)
	var totalCost = office + expense1;		//총비용 (a)
	var ratio = (totalCost / sales) * 100;	//지출비용 (b)
	
	this.ds_calc.setColumn(0,"매출합계", sales);
	this.ds_calc.setColumn(0,"카드수입총액", remaining);
	this.ds_calc.setColumn(0,"회사입금액",deposit);
	this.ds_calc.setColumn(0,"최종입금액",final);
	this.ds_calc.setColumn(0,"차액", difference);
	this.ds_calc.setColumn(0,"매출총액", total);
	this.ds_calc.setColumn(0,"총지출경비", totalCost);
	this.ds_calc.setColumn(0,"지출비율", ratio);
	comSys.showMsg("집계가 완료되었습니다. \n\n※각 항목을 확인하세요.","I");
};

