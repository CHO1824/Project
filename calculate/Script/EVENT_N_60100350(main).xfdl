		}
		Fix_Date = "";
		var eventSt = this.ds_information.getColumn(this.ds_information.rowposition,"행사상태");
		this.ds_in_Fix.setColumn(nRow,"행사상태",eventSt);
	}
	
	if(!comSys.showMsg(obj.text+" 하시겠습니까?","Q"))  return;
	
	var eventCd = this.ds_information.getColumn(this.ds_information.rowposition,"행사번호");
	var cntrDtCd = this.ds_list.getColumn(this.ds_list.rowposition,"계약상세코드");
	var custCd = this.ds_information.getColumn(this.ds_information.rowposition,"회원코드");
	
	this.ds_in_Fix.setColumn(nRow,"행사코드",eventCd);
	this.ds_in_Fix.setColumn(nRow,"계약상세코드",cntrDtCd);
	this.ds_in_Fix.setColumn(nRow,"회원코드",custCd);
	this.ds_in_Fix.setColumn(nRow,"정산완료일",Fix_Date);
	
	this.fsp_clear();
	//P_장례정산_확정
	this.fsp_addSave(
		"kwoo/event:EVENT_N_60100350_INFO"		// insert
		, "kwoo/event:EVENT_N_60100350_INFO"		// update
		, ""									// delete
		, ""									// flag column 명
		, ""									// key sql id
		, 0										// key 증가 값
		, "" 
		, 0 
		, "N"									// 실행타입 프로시저를 호출할때는 N, fsp에 직접쓸땐 B
	);
	
	// 서버 호출 
	this.fsp_callService(
		""
		,""
		,"ds_input=ds_in_Fix:U"	// inDs 
		,""	// outDs
		,""	// args
		,"fn_afterSave" //콜백
		,false
	);	
